{
  "name": "expense-tracker-backend",
  "version": "1.0.0",
  "description": "Expense Tracker API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "knex migrate:latest --knexfile src/db/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/db/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/db/knexfile.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/pino": "^7.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
